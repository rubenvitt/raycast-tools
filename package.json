{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-tools",
  "title": "Raycast Tools",
  "description": "Rubeens Raycast Tools",
  "icon": "command-icon.png",
  "author": "rubeen",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "move-to-inbox",
